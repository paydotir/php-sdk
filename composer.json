{
    "name": "paydotir/php-sdk",
    "description": "Pay.ir PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Vaziry",
            "email": "sa.vaziry@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Payir\\SDK\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.5 || ^9.3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Payir\\SDK\\Tests\\": "./tests"
        }
    },
    "minimum-stability": "dev"
}
